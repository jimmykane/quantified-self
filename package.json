    "@angular/fire": "^20.0.1",
    "@angular/forms": "19.2.17",
    "@angular/google-maps": "^21.0.2",
    "@angular/material": "^19.2.19",
    "@angular/material-moment-adapter": "^19.2.19",
    "@angular/platform-browser": "19.2.17",
    "@angular/platform-browser-dynamic": "19.2.17",
    "@angular/platform-server": "19.2.17",
    "@angular/router": "19.2.17",
    "@angular/service-worker": "19.2.17",
    "@googlemaps/markerclustererplus": "^1.1.0",
    "@sentry/angular": "^10.29.0",
    "@sentry/tracing": "^6.13.2",
    "@sports-alliance/sports-lib": "^5.4.24",
    "@types/pako": "^1.0.2",
    "fast-deep-equal": "^3.1.3",
    "firebase": "^12.6.0",
    "leaflet": "^1.7.1",
    "leaflet-easybutton": "^2.4.0",
    "leaflet-fullscreen": "^1.0.2",
